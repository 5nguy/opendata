       $('head').append('<meta name="viewport" content="width=device-width, initial-scale=1"/>');
    }
    window.addEventListener("load", function() {
      window.cookieconsent.initialise({
        "palette": {
          "popup": {"background": "#252e39"},
          "button": {"background": "#14a7d0"}
        },
        "layout": "basic-close",
        "elements": {
          "close": '<span aria-label="dismiss cookie message" tabindex="0" id="cookie-consent-hide">&#10006;</span>'
        },
        "content": {
          "message": Liferay.Language.get('cookie.message', ['www.disasterscharter.org']),
          "dismiss": Liferay.Language.get('ok'),
          "link": Liferay.Language.get('learn-more'),
          "href": "terms-and-conditions"
        }
      })

      $('#cookie-consent-hide').click(function() {
        $(".cc-window")
        .one("transitionend webkitTransitionEnd oTransitionEnd MSTransitionEnd",
         function(e){
            $(this).css('display', 'none');
         });
        $('.cc-window').css('opacity', '0');
      });

      if($('.container-fluid.page-con')) {
        $('.topper-bg').css('height', $('.container-fluid.page-con').position().top + 120);
      }
      $('#footer-current-year').text((new Date()).getFullYear());
      $('.main-navigation li').each(function() {
        if(window.location.href.indexOf($(this).find('a').attr('href')) > -1) {
          $(this).addClass('selected');
        }
      });
    });
  </script>
  


















































